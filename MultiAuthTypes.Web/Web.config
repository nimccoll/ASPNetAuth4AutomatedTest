<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="ida:ClientId" value="{your Azure AD Application Client ID here}" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/" />
    <add key="ida:Domain" value="{your Azure AD tenant name here}" />
    <add key="ida:TenantId" value="{your Azure AD tenant ID here}" />
    <add key="ida:PostLogoutRedirectUri" value="https://localhost:44329/signin-oidc" />
    <add key="ida:Authority" value="https://login.microsoftonline.com/{your Azure AD tenant ID here}/v2.0" />
    <add key="ida:Audience" value="" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.8.1" />
    <httpRuntime targetFramework="4.8.1" />
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
<!--- Enable JwtTokenValidationHttpModule to support Bearer token authentication for automated testing -->    
